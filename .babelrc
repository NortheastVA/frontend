{
    "presets": [["env", {
      "useBuiltIns": true
    }]],
    "plugins": ["transform-class-properties",
      ["transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ],
      ["module-resolver", {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "pages": "./src/pages",
          "ducks": "./src/ducks",
          "sagas": "./src/sagas",
          "utils": "./src/utils",
          "styles": "./src/styles"
        }
      }
    ]]
}
